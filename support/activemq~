#!/bin/sh
case $1 in
    start)
	/opt/apache-activemq-5.2.0/bin/linux-x86-32/activemq start
	;;
    stop)
	/opt/apache-activemq-5.2.0/bin/linux-x86-32/activemq stop
	;;
    restart)
	/opt/apache-activemq-5.2.0/bin/linux-x86-32/activemq restart
	;;
    force-reload)
	/opt/apache-activemq-5.2.0/bin/linux-x86-32/activemq restart
	;;
    status)
	/opt/apache-activemq-5.2.0/bin/linux-x86-32/activemq status
	;;
    *)
	echo "no habal $@"
	exit 1
	;;
esca
exit 0